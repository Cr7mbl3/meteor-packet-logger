{
  "schemaVersion": 1,
  "id": "meteor-packet-logger",
  "version": "${version}",
  "name": "Meteor Packet Logger",
  "description": "Addon for meteor client to log network packets to console",
  "authors": [
    "Cr7mbl3"
  ],
  "contact": {
    "repo": "https://github.com/Cr7mbl3/meteor-packet-logger"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.cr7mbl3.packetlogger.PacketLoggerAddon"
    ]
  },
  "custom": {
    "meteor-client:color": "0,204,0",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
